#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/f/Python/jira-clon-nextjs/node_modules/.pnpm/drizzle-kit@0.18.1/node_modules/drizzle-kit/node_modules:/mnt/f/Python/jira-clon-nextjs/node_modules/.pnpm/drizzle-kit@0.18.1/node_modules:/mnt/f/Python/jira-clon-nextjs/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/f/Python/jira-clon-nextjs/node_modules/.pnpm/drizzle-kit@0.18.1/node_modules/drizzle-kit/node_modules:/mnt/f/Python/jira-clon-nextjs/node_modules/.pnpm/drizzle-kit@0.18.1/node_modules:/mnt/f/Python/jira-clon-nextjs/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../node_modules/.pnpm/drizzle-kit@0.18.1/node_modules/drizzle-kit/index.js" "$@"
else
  exec node  "$basedir/../../../../node_modules/.pnpm/drizzle-kit@0.18.1/node_modules/drizzle-kit/index.js" "$@"
fi
